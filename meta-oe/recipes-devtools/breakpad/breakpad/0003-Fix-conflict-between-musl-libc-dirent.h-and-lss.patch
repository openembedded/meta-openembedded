 1 file changed, 2 insertions(+), 1 deletion(-)

--- a/src/client/linux/crash_generation/crash_generation_server.cc
+++ b/src/client/linux/crash_generation/crash_generation_server.cc
@@ -31,7 +31,6 @@
 #endif
 
 #include <assert.h>
-#include <dirent.h>
 #include <fcntl.h>
 #include <limits.h>
 #include <poll.h>
@@ -52,6 +51,8 @@
